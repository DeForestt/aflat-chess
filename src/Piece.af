class Piece{
    contract{
        int x;
        int y;
        int color;
        adr face;
        adr selectedFace;

        adr board;
        adr die;
        adr draw;
        adr move;
        adr forceMove;
    };
};

export int killPiece(Piece piece){
    piece.x = -1;
    piece.y = -1;
    return 0;
};

export int forceMove(Piece piece, int x, int y){
    piece.x = x;
    piece.y = y;
    return 0;
};